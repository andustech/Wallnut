  <script type="text/javascript">

    //initiating jQuery
    jQuery(function($) {
      if ($(window).width() >= 981) {
        $(document).ready( function() {
          $('.header-wrapper').stickUp();
        });
      } else {
        $(document).ready( function() {
          $('header').stickUp();
        });
      }

      // Gm menu code
      var check = false;
  	  var ga_interval = setInterval(checkApp, 100);
      function checkApp() {
        if (typeof GloboMenuConfig == 'object' && $('.gm-menu').length) {
          check = true;
          clearInterval(ga_interval);
        }
        if (check) {
          $('.MobileNavIcon__NavIcon-sc-xv1kjn-1').click(function(){
            if(!$('.globo-menu-mobile').hasClass('active')){
              $('.globo-menu-mobile').addClass('active');
            }else{
              $('.globo-menu-mobile').removeClass('active');
            }
          })
        } 
      }

      // Made submenu heading color white to hide it
      const myInterval = setInterval(function() {
      	$('.gm-menu .gm-heading span:contains(Default)').parent().addClass('default-title');
        if ($('.gm-menu .gm-heading span').parent().hasClass('default-title')) {
          clearInterval(myInterval);
        }
      }, 200);

      // Added to set position of gm submenu
      $(window).scroll(function () {
        menuCartDrawerPosition();
      });
      menuCartDrawerPosition();

      function menuCartDrawerPosition() {
        if ($(window).scrollTop() > 10) {
          $(".gm-submenu.gm-mega.gm-submenu-bordered").css({ "top": "75px" });
          if ($(window).width() >= 1024) {
            $("#CartDrawer").css({ "top": "75px" });
          } else {
            $("#CartDrawer").css({ "top": "50px" });
          }
        }
        else {
          $(".gm-submenu.gm-mega.gm-submenu-bordered").css({ "top": "119px" });
          if ($(window).width() >= 1024) {
            $("#CartDrawer").css({ "top": "119px" });
          } else if ($(window).width() >= 769) {
            $("#CartDrawer").css({ "top": "94px" });
          } else {
            $("#CartDrawer").css({ "top": "88px" });
          }
        }
      }

      // Show close on cart drawer click
      $('.site-header__cart-toggle').on('click', function() {
        if ($(window).width() < 767) {
          if ($('.toggle-button').hasClass('flex')) {
            $('.toggle-button').removeClass('flex');
            $('.toggle-button').addClass('hidden');
            $('.cart-close').removeClass('hidden');
          } else {
            $('.toggle-button').removeClass('hidden');
            $('.toggle-button').addClass('flex');
            $('.cart-close').addClass('hidden');
          }
        }
      });
      $('.cart-close').on('click', function() {
        if ($(window).width() < 767) {
          $('.toggle-button').removeClass('hidden');
          $('.toggle-button').addClass('flex');
          $('.cart-close').addClass('hidden');
          timber.RightDrawer.close();
        }
      });

      // Set autocomplete off for the Klaviyo newsletter input
      const myIntervalNewsletter = setInterval(function() {
      	if ($('body form.klaviyo-form').length > 0) {
          $('body form.klaviyo-form').attr('autocomplete', 'off');
          clearInterval(myIntervalNewsletter);
        }
      }, 200);

      // Set menu link as active if that page is curretnly open
      const myIntervalMenuLinkActive = setInterval(function() {
        if ($('.gm-menu').length > 0) {
          var curr_href = window.location.pathname;
          $('.navbar .gm-menu:not(.gm-menu-mobile) .gm-item.gm-level-0').each(function(){
              if ($(this).find('a').attr('href') != '/' && $(this).find('a').attr('href') == curr_href) {
                $(this).addClass('link-active');
                clearInterval(myIntervalMenuLinkActive);
              }
          });
        }
      }, 200);

      // Set height of main banner based on viewport
      function bannerImageHeight() {
        if ($(window).width() >= 991 && $('body').hasClass('index')){
          const myIntervalBannerheight = setInterval(function() {
            if ($('body .gm-menu').length > 0) {
              var ann_height = $('#react-header').outerHeight();
              var header_height = $('#react-desktop-announcement').outerHeight();
              var total_height = ann_height + header_height;
              $('.main_banner').css('height', 'calc(100vh - '+total_height+'px)');
              
              var windowWidth = $(window).width();
              if (windowWidth > 1440) {
                $('.main_banner').css('max-height', (windowWidth/2)+'px');
              }
              else {
                $('.main_banner').css('max-height', 'none');
              }

              $('.main_banner img').hide()
              clearInterval(myIntervalBannerheight);
            }
          }, 200);
        } else {
          $('.main_banner img').show()
          $('.main_banner').css('height', 'auto');
        }
      }
      bannerImageHeight();

      $( window ).resize(function() {
        bannerImageHeight();
        menuCartDrawerPosition();
      });

      // Search layer and auto suggestion code for desktop and mobile
      function searchLayer() {
        $('.search-click').on('click', function(){
          if ($(window).width() >= 1024) {
            $(this).hide();
            $('.search-form').addClass('active');
            $(this).parent().siblings('form').find('input').focus();
          } else if ($(window).width() < 1024) {
            $('.search-form-mob').addClass('active');
            $('.search-mob-div').slideToggle();
            $('.search-form-mob').find('input').focus();
            if ($(this).hasClass('search')) {
              $(this).attr('src', 'https://cdn.shopify.com/s/files/1/0627/3476/2207/files/close-img-black.png?v=1654240702');
              $(this).addClass('close');
              $(this).removeClass('search');
            } else {
              $(this).attr('src', 'https://cdn.shopify.com/s/files/1/0627/3476/2207/files/search-icon.svg?v=1652783584');
              $(this).addClass('search');
              $(this).removeClass('close');
            }
          }
        });
      } 
      searchLayer();

      $(document).on('click', function (e) {
        if ($(window).width() >= 1024) {
          if (
            !(
              $(e.target).closest('.search-form').length > 0 ||
              $(e.target).closest('.search__results-wrapper').length > 0 ||
              $(e.target).closest('.search-click').length > 0
            )
          ) {
            $('.search-form').removeClass('active');
            $('.search-click').show();
          }
        } else if ($(window).width() < 1024) {
          if (
            !(
              $(e.target).closest('.search-mob-div').length > 0 ||
              $(e.target).closest('.search__results-wrapper').length > 0 ||
              $(e.target).closest('.search-click').length > 0 ||
              $(e.target).closest('button.toggle-button').length > 0   
            )
          ) {
            $('.search-mob-div').slideUp();
            $('.search-click').attr('src', 'https://cdn.shopify.com/s/files/1/0627/3476/2207/files/search-icon.svg?v=1652783584');
            $('.search-click').addClass('search');
            $('.search-click').removeClass('close');
          }
        }
      });

      $( window ).resize(function() {
        if ($(window).width() >= 1024) {
          $('.search-form').removeClass('active');
          $('.search-click').show();
        } else if ($(window).width() < 1024) {
          $('.search-mob-div').slideUp();
          $('.search-click').attr('src', 'https://cdn.shopify.com/s/files/1/0627/3476/2207/files/search-icon.svg?v=1652783584');
          $('.search-click').addClass('search');
          $('.search-click').removeClass('close');
        }
      });
      var shopURL = $('body').data('shop-url');
      var currentAjaxRequest = null;
            var searchPath = shopURL + '/search?q=';
      var searchForms = $('form.search_form, form.search-form, form.search, form.header_search_form').each(function() {
        var input = $(this).find('input[name="q"]');

        // Adding a list for showing search results.
        $('<div class="search__results-wrapper"><ul class="search__results"></ul></div>').appendTo($(this));
        // Listening to keyup and change on the text field within these search forms.
        input.attr('autocomplete', 'off').bind('keyup change', function() {
          if(input.val() == '') {
            $('.search__results').hide();
            $('.search__results').empty();
          }

          var term = $(this).val();
          var cleanTerm = encodeURI(term);

          var form = $(this).closest('form');
          var searchURL =  searchPath + cleanTerm;
          var resultsList = form.find('.search__results');

          if (term.length >= 3) {

            if (currentAjaxRequest != null) currentAjaxRequest.abort();
            currentAjaxRequest = $.getJSON(searchURL +'*&view=json', function(data) {
              resultsList.empty();
              // If we have no results.
              if(data.results_count == 0) {
                resultsList.hide();
                // resultsList.append('<p>No items found </p>');
              } else {
                // If we have results.
                $.each(data.results, function(index, item) {
                  if(index >= 5) {
                    return false;
                  }

                  var link = $('<a></a>').attr('href', item.url);

                  //Collect collection handles if product
                  if(item.object_type == 'product') {

                    var collectionHandles = [];
                    for(i=0; i < item.collections.length; i++) {
                      collectionHandles.push(item.collections[i].handle);
                    }

                    if($.inArray('coming-soon', collectionHandles) != -1) {
                      var itemPrice = Shopify.translation.coming_soon_text;
                    } else {
                      if(item.available == true) {

                        if(item.raw_compare > item.raw_price ) {
                          var itemPrice = '<span class="was_price">' + item.compare + '</span> ' + item.price;
                        } else {
                          if(item.price_varies && item.price_min > 0) {
                            var itemPrice = 'from' + ' ' + item.price;
                          } else if(item.price > 0 || item.raw_price > 0) {
                            var itemPrice = item.price;
                          } else {
                            var itemPrice = Shopify.theme_settings.free_text;
                          }
                        }
                      } else {
                        var itemPrice = Shopify.theme_settings.display_sold_out_price ? item.price + ' ' + '<span class="sold-out-text">' + Shopify.theme_settings.sold_out_text + '</span>' : '<span class="sold-out-text">' + Shopify.theme_settings.sold_out_text + '</span>';
                      }
                    }

                    if(item.thumbnail != 'NULL') {
                      link.append('<div class="thumbnail mb-0"><img class="lazyload {{ settings.image_loading_style }}" src="' + item.thumbnail + '" /></div>');
                    }
                    link.append('<p class="title mb-0">' + item.title + '<br><p class="item-pricing hidden">'+ itemPrice +'</p></p>');


                  } else if(item.object_type == 'article') {
                    if(item.thumbnail != 'NULL') {
                      link.append('<div class="thumbnail"><img class="lazyload {{ settings.image_loading_style }}" src="' + item.thumbnail + '" /></div>');
                    }
                    link.append('<div class="title">' + item.title + '<br><span class="item-pricing">'+ item.text_content +'</span></div>');

                  } else if(item.object_type == 'page') {
                    link.append('<div class="title">' + item.title + '<br><span class="item-pricing">'+ item.text_content +'</span></div>');
                  }

                  var border_padding = '';
                  if(index > 0) {
                    border_padding = 'border-t py-4';
                  } else {
                    border_padding = 'pb-4';
                  }
                  link.wrap('<li class="item-result result-- ' + border_padding + ' ' + item.object_type + '"></li>');
                  resultsList.append(link.parent());
                });
                // The Ajax request will return at the most 5 results.
                // If there are more than 5, let's link to the search results page.
                if(data.results_count >= 5) {
                  resultsList.append('<li class="all-results border-t py-4"><span class="title see-all"><a href="' + searchURL + '*">' + 'View all results' + ' (' + data.results_count + ')</a></span></li>');
                }
                resultsList.fadeIn(200);
              }
            })
          }
        });
      });
      // Clicking outside makes the results disappear.
      $(document).on('click', function(e){
        var container = $('[data-autocomplete-true]');
        if (!container.is(e.target) && container.has(e.target).length === 0) {
          $('[data-autocomplete-true]').find('.search__results-wrapper').hide();
        }
      });

      $('[data-autocomplete-true] input').on('keyup', function(){
        if ($(this).val() != '') {
          $('.search__results-wrapper').hide();
          $(this).parents('[data-autocomplete-true]').find('.search__results-wrapper').show();
        }
      });

    });

  </script>

