  <script>
    jQuery(document).on('mouseenter', '.list-img-wrap', function(){
      jQuery(this).find(".img-wrap").each(function(){
        if(jQuery(this).hasClass("hover")) {
          jQuery(this).animate({
            'opacity': 1
          }, "slow");
        } else {
          jQuery(this).animate({
            'opacity': 0
          }, "slow")
        }
      });
    });
    jQuery(document).on('mouseleave', '.list-img-wrap', function(){
      jQuery(this).find(".img-wrap").each(function(){
        if(jQuery(this).hasClass("hover")) {
          jQuery(this).animate({
            'opacity': 0
          }, "slow");
        } else {
          jQuery(this).animate({
            'opacity': 1
          }, "slow")
        }
      });
    });
    jQuery(document).on('touchstart', '.list-img-wrap', function(){
      jQuery(this).find(".img-wrap").each(function(){
        if(jQuery(this).hasClass("hover")) {
          jQuery(this).animate({
            'opacity': 1
          }, "slow");
        } else {
          jQuery(this).animate({
            'opacity': 0
          }, "slow")
        }
      });
    });
    jQuery(document).on('touchend', '.list-img-wrap', function(){
      jQuery(this).find(".img-wrap").each(function(){
        if(jQuery(this).hasClass("hover")) {
          jQuery(this).animate({
            'opacity': 0
          }, "slow");
        } else {
          jQuery(this).animate({
            'opacity': 1
          }, "slow")
        }
      });
    });
